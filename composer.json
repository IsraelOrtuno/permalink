{
    "name": "devio/permalink",
    "description": "Permalink routing system for Laravel. Advanced database driven routes. Handle your permalinks + SEO parameters directly from database.",
    "keywords": [
        "permalink",
        "laravel",
        "routing",
        "database routing",
        "dynamic routes",
        "wordpress"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Israel Ortu√±o",
            "email": "ai.ortuno@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.6.*|5.7.*|5.8.*",
        "illuminate/database": "5.6.*|5.7.*|5.8.*",
        "illuminate/routing": "5.6.*|5.7.*|5.8.*",
        "cviebrock/eloquent-sluggable": "^4.5",
        "arcanedev/seo-helper": "^1.5.0",
        "divineomega/array_undot": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Devio\\Permalink\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Devio\\Permalink\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
            },
            "providers": [
                "Devio\\Permalink\\PermalinkServiceProvider"
            ]
        }
    }
}
