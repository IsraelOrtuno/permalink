{
    "name": "devio/permalink",
    "description": "Permalink routing system for Laravel. Advanced database driven routes. Handle your permalinks + SEO parameters directly from database.",
    "keywords": [
        "permalink",
        "laravel",
        "routing",
        "database routing",
        "dynamic routes",
        "wordpress"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Israel Ortu√±o",
            "email": "ai.ortuno@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "cviebrock/eloquent-sluggable": "^6.0|^7.0|^8.0",
        "arcanedev/seo-helper": "^2.0|^3.0|^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "~8.0",
        "mockery/mockery": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Devio\\Permalink\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Devio\\Permalink\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
            },
            "providers": [
                "Devio\\Permalink\\PermalinkServiceProvider"
            ]
        }
    }
}
